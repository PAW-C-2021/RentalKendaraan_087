@model RentalKendaraan_087.Models.Pengembalian

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Pengembalian</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayName("Tanggal Pengembalian")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TglPengembalian)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayName("Denda")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Denda)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayName("Kondisi")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdKondisiNavigation.NamaKondisi)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayName("Tanggal Peminjaman")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdPeminjamanNavigation.TglPeminjaman)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdPengembalian">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
